   "source": [
    "### 带参数的装饰器"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "<p>小明加油</p>\n"
     ]
    }
   ],
   "source": [
    "def web(ty):\n",
    "    def web_re(func):\n",
    "        def web_func(name):\n",
    "            print('<{0}>{1}</{0}>'.format(ty,func(name)))\n",
    "        return web_func\n",
    "    return web_re\n",
    "@web('p')\n",
    "def fun(name):\n",
    "    return name+'加油'\n",
    "\n",
    "fun('小明')\n",
    "        "
   ]
